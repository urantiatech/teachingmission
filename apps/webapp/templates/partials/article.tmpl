<div class="row">
	<div class="col s12">
		<div class="card grey lighten-2">
			<div class="card-content">

			{{ with .Article }}
				<h1>{{ .Title | title}}</h1>
				<span class="caption">{{  .Text }}</span>
			{{ end }}

			{{ if not .Article }}
				<h1>Page Not Found</h1>
			{{ end }}

			</div>
		</div>
	</div>
</div>